{
  "name": "page-builder-example-jekyll",
  "private": true,
  "description": "A page builder example for Jekyll and Bookshop, tailored to CloudCannon",
  "scripts": {
    "bookshop-dev": "bookshop-browser -b component-library -p 6061",
    "bookshop-hosted": "bookshop-browser -b component-library -o site/assets/bookshop-hosted.js",
    "bookshop-live": "bookshop-live -b component-library -o site/_cloudcannon/bookshop-live.js",
    "jekyll": "bundle exec jekyll serve --port 6060 --trace --unpublished",
    "start": "run-p bookshop-dev jekyll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CloudCannon/page-builder-example-jekyll.git"
  },
  "author": "CloudCannon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CloudCannon/page-builder-example-jekyll/issues"
  },
  "homepage": "https://github.com/CloudCannon/page-builder-example-jekyll#readme",
  "devDependencies": {
    "@bookshop/browser": "^2.2.0",
    "@bookshop/jekyll-engine": "^2.2.0",
    "@bookshop/live": "^2.2.0",
    "npm-run-all": "^4.1.5"
  }
}
